    <stylesheets>
        <URL value="@movieplayerfxml.css" />
    </stylesheets>
   <children>
      <MediaView fx:id="moviePlayer" fitHeight="355.0" fitWidth="600.0" layoutX="3.0" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
      <Button fx:id="btn_back" layoutX="216.0" layoutY="355.0" mnemonicParsing="false" onAction="#backMovie" text="Back" />
      <Button fx:id="btn_play" layoutX="287.0" layoutY="355.0" mnemonicParsing="false" onAction="#playMovie" text="Play" />
      <Button fx:id="btn_pause" layoutX="364.0" layoutY="355.0" mnemonicParsing="false" onAction="#pauseMovie" text="Pause" />
   </children>
</AnchorPane>
